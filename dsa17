class Solution{
public:
    string removeDuplicates(string str) {
        // code here
        string ans = "";
        for(int i=0;i<str.length();i++){
            
             if(ans.length()==0) ans.push_back(str[i]);
             
             if(ans.find(str[i])==string::npos){
                 ans.push_back(str[i]);
             }
             else{
                 
                 
             }
        }
        return ans;
    }
};
